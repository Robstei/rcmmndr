    "@types/spotify-web-playback-sdk": "^0.1.16",
    "autoprefixer": "10.4.14",
    "cookies-next": "^3.0.0",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.3",
    "gray-matter": "^4.0.3",
    "next": "^13.4.19",
    "next-auth": "^4.23.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "zod": "^3.22.2",
