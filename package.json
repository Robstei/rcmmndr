  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "migrate:dev": "npx dotenv-cli -e .env.local -- npx prisma migrate dev --schema ./src/prisma/schema.prisma",
